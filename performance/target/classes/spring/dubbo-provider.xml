<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="performance_provider" owner="zhouxi.zhou" organization="performance"/>

    <dubbo:registry address="10.17.2.151:2181,10.17.2.151:2182,10.17.2.151:2183" group="performance"
                    protocol="zookeeper" id="performanceZk" default="true"/>

    <dubbo:protocol name="dubbo" port="20110" accesslog="true" threads="500"/>


    <dubbo:service interface="com.xn.performance.service.PerformanceStressMachineService"
                   ref="performanceStressMachineService" version="1.0.0"
                   timeout="60000" registry="performanceZk"/>

    <!-- 和本地bean一样实现服务 -->
    <bean id="performanceStressMachineService"
          class="com.xn.performance.service.impl.PerformanceStressMachineServiceImpl"/>
</beans>
