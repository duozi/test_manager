<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.xn.autotest.dao.DubboServicePropertiesMapper">

    <resultMap id="DubboServicePropertiesMap" type="com.xn.autotest.bean.properties.dubbo.entity.DubboServiceProperties">
        <result property="id" column="id"/>
        <result property="interfaceId" column="interface_id"/>
        <result property="appName" column="app_name"/>
        <result property="interfaceName" column="interface_name"/>
        <result property="url" column="url"/>
        <result property="groupName" column="group_name"/>
        <result property="version" column="version"/>
        <result property="timeout" column="timeout"/>
        <result property="useZk" column="use_zk"/>
        <result property="zk" column="zk"/>
        <result property="dubboStatus" column="dubbo_status"/>
        <result property="changeTime" column="change_time"/>
    </resultMap>
    
    <sql id="columns">
        <![CDATA[
        id, interface_id, app_name, interface_name, url, group_name, version, timeout, use_zk, zk, dubbo_status, change_time
        ]]>
    </sql>
    
    <sql id="where">
        <where>
           <if test="@Ognl@isNotEmpty(id)">
                AND id = #{id}
            </if>
           <if test="@Ognl@isNotEmpty(interfaceId)">
                AND interface_id = #{interfaceId}
            </if>
           <if test="@Ognl@isNotEmpty(appName)">
                AND app_name = #{appName}
            </if>
           <if test="@Ognl@isNotEmpty(interfaceName)">
                AND interface_name = #{interfaceName}
            </if>
           <if test="@Ognl@isNotEmpty(url)">
                AND url = #{url}
            </if>
           <if test="@Ognl@isNotEmpty(groupName)">
                AND group_name = #{groupName}
            </if>
           <if test="@Ognl@isNotEmpty(version)">
                AND version = #{version}
            </if>
           <if test="@Ognl@isNotEmpty(timeout)">
                AND timeout = #{timeout}
            </if>
           <if test="@Ognl@isNotEmpty(useZk)">
                AND use_zk = #{useZk}
            </if>
           <if test="@Ognl@isNotEmpty(zk)">
                AND zk = #{zk}
            </if>
           <if test="@Ognl@isNotEmpty(dubboStatus)">
                AND dubbo_status = #{dubboStatus}
            </if>
           <if test="@Ognl@isNotEmpty(changeTimeBegin)">
                AND change_time >= #{changeTimeBegin}
           </if>
           <if test="@Ognl@isNotEmpty(changeTimeEnd)">
                AND change_time &lt;= #{changeTimeEnd}
           </if>
        </where>
    </sql>
    
    <select id="get" resultMap="DubboServicePropertiesMap">
        SELECT <include refid="columns" />
        <![CDATA[
            FROM
                dubbo_service_properties
            WHERE 
                id = #{id} 
        ]]>
    </select>
    
    <select id="list" resultMap="DubboServicePropertiesMap">
        SELECT
            <include refid="columns" />
        FROM
            dubbo_service_properties 
        <include refid="where"/>
    </select>
        
    <select id="count" resultType="long">
        <![CDATA[
            SELECT
                count(1)
            FROM
                dubbo_service_properties
        ]]>
        <include refid="where"/>
    </select>

    <insert id="save" useGeneratedKeys="true" keyProperty="id">
        <![CDATA[
            INSERT INTO dubbo_service_properties (
                id,
                interface_id,
                app_name,
                interface_name,
                url,
                group_name,
                version,
                timeout,
                use_zk,
                zk,
                dubbo_status,
                change_time
            ) VALUES (
                #{id},
                #{interfaceId},
                #{appName},
                #{interfaceName},
                #{url},
                #{groupName},
                #{version},
                #{timeout},
                #{useZk},
                #{zk},
                #{dubboStatus},
                #{changeTime}
            )
        ]]>
    </insert>

    <insert id="saveBatch">
        <![CDATA[
            INSERT INTO dubbo_service_properties (
                id,
                interface_id,
                app_name,
                interface_name,
                url,
                group_name,
                version,
                timeout,
                use_zk,
                zk,
                dubbo_status,
                change_time
            ) VALUES
        ]]>
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.id},#{item.interfaceId},#{item.appName},#{item.interfaceName},#{item.url},#{item.groupName},#{item.version},#{item.timeout},#{item.useZk},#{item.zk},#{item.dubboStatus},#{item.changeTime})
        </foreach>
    </insert>
    
    <update id="update">
        	UPDATE
                dubbo_service_properties
            <set>
                <if test="interfaceId != null">
                	<![CDATA[ interface_id = #{interfaceId} ]]>,
                </if>
                <if test="appName != null">
                	<![CDATA[ app_name = #{appName} ]]>,
                </if>
                <if test="interfaceName != null">
                	<![CDATA[ interface_name = #{interfaceName} ]]>,
                </if>
                <if test="url != null">
                	<![CDATA[ url = #{url} ]]>,
                </if>
                <if test="groupName != null">
                	<![CDATA[ group_name = #{groupName} ]]>,
                </if>
                <if test="version != null">
                	<![CDATA[ version = #{version} ]]>,
                </if>
                <if test="timeout != null">
                	<![CDATA[ timeout = #{timeout} ]]>,
                </if>
                <if test="useZk != null">
                	<![CDATA[ use_zk = #{useZk} ]]>,
                </if>
                <if test="zk != null">
                	<![CDATA[ zk = #{zk} ]]>,
                </if>
                <if test="dubboStatus != null">
                	<![CDATA[ dubbo_status = #{dubboStatus} ]]>,
                </if>
                <if test="changeTime != null">
                	<![CDATA[ change_time = #{changeTime} ]]>
                </if>
            </set>
            WHERE
                id = #{id} 
    </update>
    
    <delete id="deleteByPK">
        <![CDATA[
            DELETE
            FROM
                dubbo_service_properties
            WHERE
                id = #{id} 
        ]]>
    </delete>
    
    <delete id="delete">
        <![CDATA[
            DELETE
            FROM
                dubbo_service_properties
        ]]>
        <include refid="where"/>
    </delete>
    
    <delete id="deleteBatchByPK">
        <![CDATA[
            DELETE
            FROM
                dubbo_service_properties
            WHERE
                id IN
        ]]>
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">#{ids}</foreach>
    </delete>
</mapper>

